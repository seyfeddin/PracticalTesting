# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'PracticalTesting' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Moya'
  pod 'ObjectMapper'
  pod 'SwiftyJSON'
  pod 'Kingfisher'

  # Pods for PracticalTesting



  target 'PracticalTestingUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'PracticalTestingTests' do
    inherit! :search_paths

    use_frameworks!

    # Pods for testing

    pod 'iOSSnapshotTestCase'
    pod 'Quick'
    pod 'Nimble'
    pod 'ObjectMapper'

end
